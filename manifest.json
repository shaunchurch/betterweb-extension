{
  "manifest_version": 2,
  "name": "Betterweb",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "web_accessible_resources": ["images/*.png", "images/*.svg"]
}
